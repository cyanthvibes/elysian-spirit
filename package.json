{
  "name": "elysian-spirit",
  "version": "1.0.0",
  "description": "Elysian Spirit, Discord bot for the Old School RuneScape (OSRS) Elysium clan",
  "license": "MIT",
  "type": "module",
  "module": "src/index.ts",
  "main": "dist/index.js",
  "scripts": {
    "dev": "NODE_ENV=development tsx watch src/index.ts",
    "prod": "NODE_ENV=production node ./dist/index.js",
    "build": "rm -rf dist && tsc --build --verbose ./ && tsc-alias",
    "build:full": "npm run lint:fix && npm run prettier:fix && npm run build",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "prettier": "prettier src --check",
    "prettier:fix": "prettier src --write",
    "register-guild-dev": "NODE_ENV=development tsx src/scripts/registerToDiscord.ts guild",
    "register-global-dev": "NODE_ENV=development tsx src/scripts/registerToDiscord.ts global",
    "register-guild-prod": "FORCE_TS=true NODE_ENV=production tsx src/scripts/registerToDiscord.ts guild",
    "register-global-prod": "FORCE_TS=true NODE_ENV=production tsx src/scripts/registerToDiscord.ts global",
    "clear-guild-dev": "NODE_ENV=development tsx src/scripts/registerToDiscord.ts guild clear",
    "clear-global-dev": "NODE_ENV=development tsx src/scripts/registerToDiscord.ts global clear",
    "clear-guild-prod": "FORCE_TS=true NODE_ENV=production tsx src/scripts/registerToDiscord.ts guild clear",
    "clear-global-prod": "FORCE_TS=true NODE_ENV=production tsx src/scripts/registerToDiscord.ts global clear"
  },
  "dependencies": {
    "@eslint/js": "^9.35.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/client": "^6.16.2",
    "chrono-node": "2.8.4",
    "csv-parse": "^6.1.0",
    "discord.js": "^14.22.1",
    "dotenv": "^17.2.2",
    "emoji-regex": "^10.5.0",
    "glob": "^11.0.3",
    "googleapis": "^159.0.0",
    "luxon": "^3.7.2",
    "node-fetch": "^3.3.2",
    "uuid": "^13.0.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/js-yaml": "^4.0.9",
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.4.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-perfectionist": "^4.15.0",
    "prettier": "^3.6.2",
    "prisma": "^6.16.1",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0"
  }
}
